{
  "name": "buchiyu",
  "version": "0.0.0",
  "private": true,
  "files": [
    ".env",
    "koishi.yml"
  ],
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "build": "koishi-scripts build",
    "clean": "koishi-scripts clean",
    "new": "koishi-scripts new",
    "setup": "koishi-scripts setup",
    "bump": "yakumo version",
    "dep": "yakumo upgrade",
    "pub": "yakumo publish",
    "dev": "cross-env NODE_ENV=development koishi start -r esbuild-register -r yml-register --watch",
    "start": "koishi start",
    "yalc-push": "koishi-scripts build && yalc push --replace",
    "watch": "nodemon --ignore lib/ --ignore node_modules/ --watch .yalc/ -C -e js --debug -x npm run yalc-push"
  },
  "yakumo": {
    "require": [
      "esbuild-register",
      "yml-register"
    ]
  },
  "devDependencies": {
    "@koishijs/client": "^4.3.7",
    "@koishijs/scripts": "^3.2.1",
    "@types/node": "^18.7.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.2",
    "esbuild-register": "^3.3.3",
    "typescript": "^4.7.4",
    "yakumo": "^0.3.3",
    "yakumo-publish": "^0.3.0",
    "yakumo-upgrade": "^0.3.1",
    "yakumo-version": "^0.3.0",
    "yml-register": "^1.1.0"
  },
  "dependencies": {
    "@koishijs/cli": "^4.8.5",
    "@koishijs/plugin-adapter-discord": "^3.0.3",
    "@koishijs/plugin-adapter-onebot": "^5.0.7",
    "@koishijs/plugin-adapter-telegram": "^3.1.3",
    "@koishijs/plugin-console": "^4.3.7",
    "@koishijs/plugin-database-mysql": "^4.5.1",
    "@koishijs/plugin-dataview": "^2.1.2",
    "@koishijs/plugin-echo": "^2.0.1",
    "@koishijs/plugin-help": "^1.1.1",
    "@koishijs/plugin-logger": "^2.1.4",
    "@koishijs/plugin-market": "^1.1.4",
    "@koishijs/plugin-repeater": "1.1.0",
    "@koishijs/plugin-sandbox": "^2.2.0",
    "@koishijs/plugin-schedule": "^4.3.0",
    "@koishijs/plugin-status": "^6.1.4",
    "koishi": "^4.8.5",
    "koishi-plugin-gocqhttp": "^3.0.3"
  }
}
